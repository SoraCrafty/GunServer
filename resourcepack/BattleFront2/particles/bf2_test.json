{
  "format_version": "1.10.0",
  "particle_effect": {
    "description": {
      "identifier": "bf2:totem_particle",
      "basic_render_parameters": {
        "material": "particles_alpha",
        "texture": "textures/items/compass_atlas"
      }
    },
    "components": {
      "minecraft:emitter_rate_steady": {
        "spawn_rate": 100,
        "max_particles": 100
      },

      "minecraft:emitter_lifetime_once": {
        "active_time": 1
      },

      "minecraft:emitter_shape_point": {
        "offset": [ 0.0, "Math.random(-0.9, -0.5)", 0.0 ],
        "direction": [ "Math.random(-1.0, 1.0)", "Math.random(-1.0, 1.0)", "Math.random(-1.0, 1.0)" ]
      },

      "minecraft:particle_initial_speed": "Math.random(10.0, 35.0)",

      "minecraft:particle_lifetime_expression": {
        "max_lifetime": "3.0 + Math.random(0.0, 0.6)"
      },

      "minecraft:particle_motion_dynamic": {//縦方向の減少
        "linear_drag_coefficient": 20
      },

      "minecraft:particle_appearance_billboard": {
        "size": [
          "(0.05 + variable.particle_random_1*0.05)",
          "(0.05 + variable.particle_random_1*0.05)"
        ],

        "facing_camera_mode": "lookat_y",

        "uv": {
          "texture_width": 16,
          "texture_height": 512,
          "flipbook": {
            "base_UV": [ 0, 0 ],
            "size_UV": [ 16, 16 ],
            "step_UV": [ 0, 16],
            "frames_per_second": 8,
            "max_frame": 32,
            "stretch_to_lifetime": false,
            "loop": true
          }
        }
      },

      "minecraft:particle_appearance_tinting": {
        "color": [1, 1, 1, 1]
      },

      "minecraft:particle_appearance_lighting": {}
    }
  }
}
